#ifndef _SERVIDORSTRUCT_H_ 
#define _SERVIDORSTRUCT_H_ 

#include "../defs.h"

typedef struct user {
	char nickname[NAME_LENGTH];
	int state;
	int sessionID; // -1 si esta libre
	time_t time;
	pid_t userID;
} user_t;

typedef struct session {
	int id;
	char name[NAME_LENGTH];
	int users;
} session_t;


#endif
